  <div id="whiskey-back">
    <div class="white">
      <h1>Sign In Page</h1>

      <%= form_tag @user do |form| %>
      	<%= label_tag :username %>
      	<%= text_field_tag :username%><br/>
      	<%= label_tag :password %>
      	<%= password_field_tag :password %><br/>
      	<%= submit_tag "Come on in!" %>
      <% end %>
  </div>
</div>