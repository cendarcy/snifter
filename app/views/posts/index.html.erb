
<h1>All Posts</h1>
<div class="feed">
    <% @posts.each do |post| %>
      <div>
      	<h2>Title:</h2>
        		<h3><%= post.title %></h3>
        	<h2>Content:</h2>	
        		<ul><%= post.text %></ul>
        	<%= link_to 'Show', post_path(post) %>	
        	<%= link_to 'Edit', edit_post_path(post) %>	
        	<%= link_to 'Destroy', post_path(post), method: :delete, data: {confirm: 'Are you sure?'} %>
      </div>
    <% end %>
</div>
