<div class="hdr">
<h1>All Posts</h1>
</div>
<div class="feed">
    <% @posts.each do |post| %>
      <div id="space-me">
      	<h2>Title:</h2>
        		<h3><%= post.title %></h3>
        	<h2>Content:</h2>	
        		<p><%= post.text %></p>
        	<h4><%= link_to 'Show', post_path(post) %>	
        	<%= link_to 'Edit', edit_post_path(post) %>	
        	<%= link_to 'Destroy', post_path(post), method: :delete, data: {confirm: 'Are you sure?'} %></h4>
      </div>
    <% end %>
</div>
